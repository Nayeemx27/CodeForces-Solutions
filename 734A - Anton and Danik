#include <iostream>
#include <string>
using namespace std;
 
int main()
{
    int x, ant=0, dan=0;
 
    cin>>x;
    string str;
    cin>>str;
 
    for (int i=0; i<x; i++)
    {
        if (str[i]=='A')
        {
            ant++;
        }
        else if (str[i]=='D')
        {
            dan++;
        }
    }
 
    if (ant>dan)
    {
        cout<<"Anton";
    }
    else if (dan>ant)
    {
        cout<<"Danik";
    }
    else cout<<"Friendship";
 
return 0;
}
