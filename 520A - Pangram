#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    int x, total;
    cin>>x;
    string str;
    cin>>str;
    for (int i=0; i<x; i++)
    {
        str[i]=towlower(str[i]);
    }
    sort (str.begin(),str.end());
    
    for (int i=0;i<x;i++)
    {
        if(str[i]!=str[i+1])
        {
            total++;
        }
    }
    if (total ==26)
        cout<<"YES";
    else cout<<"NO";
 
 
 
    return 0;
}
