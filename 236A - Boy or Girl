#include <bits/stdc++.h>
using namespace std;
 
int main()
{
    string str;
    getline(cin,str);
    sort(str.begin(),str.end());
    int x=str.size(),total=0;
 
    for (int i=0; i<x; i++)
    {
        if (str[i]!=str[i+1])
        {
            total++;
        }
    }
    if(total%2==0)
            cout<<"CHAT WITH HER!";
    else cout<<"IGNORE HIM!";
 
 
    return 0;
}
